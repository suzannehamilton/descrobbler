{
  "manifest_version": 2,

  "name": "Descrobbler",
  "description": "Find and delete duplicate scrobbles from a last.fm library page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Descrobbler"
  },
  "permissions": [
    "activeTab",
    "https://www.last.fm/*"
  ]
}
